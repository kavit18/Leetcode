class Solution {
    public int subtractProductAndSum(int n) {
        
        int product = 1;
        int count = 0;
       
        while(n>0)
        {
            product = product* (n%10);
            count = count + (n%10);
            n = n/10;
            
        }
        
        return product-count;
        
    }
}
